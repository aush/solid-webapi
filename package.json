{
  "name": "solid-webapi",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "api",
    "webapi",
    "express",
    "passport",
    "jwt",
    "mongo"
  ],
  "homepage": "https://github.com/aush/solid-webapi",
  "bugs": "https://github.com/aush/solid-webapi/issues",
  "license": "MIT",
  "author": {
    "name": "Anton Matveev",
    "email": "aush.tx@gmail.com",
    "url": "http://github.com/aush"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aush/solid-webapi.git"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "lint": "eslint src/**/*.js*",
    "prod": "better-npm-run prod",
    "start": "npm run dev",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "dev": {
      "command": "node bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "prod": {
      "command": "node bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "test": {
      "command": "mocha test/**/*.spec.js",
      "env": {
        "NODE_ENV": "test",
        "PORT": 8080
      }
    }
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.6.1",
    "mongoose": "^4.4.6",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "axios": "^0.9.1",
    "babel-eslint": "^5.0.0",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.1",
    "mocha": "^2.4.5"
  },
  "private": true
}
